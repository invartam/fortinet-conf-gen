{
    "name": "OCD/fortinet-conf-gen",
    "description": "Fortinet configuration generator",
    "keywords": ["fortinet"],
    "license": "GPL",
    "repositories":[
      {
          "type": "vcs",
          "url": "https://github.com/PHPOffice/PhpSpreadsheet.git"
      },
      {
          "type": "vcs",
          "url": "https://github.com/invartam/fortinet-conf-gen.git"
      }
    ],
    "require": {
        "phpoffice/phpspreadsheet": "dev-master",
        "OCD/fortinet-php": "dev-master"
    },
    "autoload": {
        "psr-0": {
          "Tests\\": "tests/"
        },
        "psr-4": {
          "Fortinet\\ConfGenerator\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*",
        "kherge/box": "^2.7"
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false
    }
}
